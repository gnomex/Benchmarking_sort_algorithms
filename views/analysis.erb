<section class="row-fluid">
  <section class="span">

    <section id="errors">
        <%if @error then %>
          <div class="alert alert-error"><%=@error%></div>
        <% end %>
      </section> 
  </section>

  <section>
    <h1 style="font-family: Marcellus SC, serif; font-weight: 300; text-align: center;">
      Resultados da execução
    </h1>
  </section>

  <section>
  <table class="table table-bordered table-striped">
    <thead>
        <tr>
          <th>#</th>
          <th>BubbleSort</th>
          <th>BubbleSort Enhanced</th>
          <th>SelectionSort</th>
          <th>InsertionSort</th>
          <th>HeapSort</th>
          <th>ShellSort</th>
          <th>QuickSort</th>
          <th>ShakerSort</th>
          <th>MergeSort</th>
          <th>RadixSort</th>
        </tr>
    </thead>
    <tbody>
      <% @report.each do |key, value| %>
        <tr>
          <td><%= key %></td>
          <% value.each do |algorithm, tms| %>
           <td>
            <%= tms.to_a.last %>
           </td>
          <% end %>
        </tr>
      <% end %>
    </tbody>
  </table>
      <span class="label label-success">*Em valores absolutos</span>
  </section>

</section>