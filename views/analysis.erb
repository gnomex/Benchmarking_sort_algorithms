<section class="row-fluid">
  <section class="span">

    <section id="errors">
        <%if @error then %>
          <div class="alert alert-error"><%=@error%></div>
        <% else %>
          <p class="alert alert-success">File is: <strong><%= @file_name %></strong></p>
        <% end %>
      </section> 
  </section>

  <section class="" id="file-contents">
  <table class="table table-bordered table-striped">
    <thead>
        <tr>
        %w(bubble_sort bubble_sort_enhanced selection_sort insertion_sort heapsort
      shell_sort quicksort mergesort shakersort)
          <th># Arquivo</th>
          <th>BubbleSort</th>
          <th>BubbleSort Enhanced</th>
          <th>SelectionSort</th>
          <th>InsertionSort</th>
          <th>HeapSort</th>
          <th>ShellSort</th>
          <th>QuickSort</th>
          <th>MergeSort</th>
          <th>ShekerSort</th>
        </tr>
    </thead>
    <tbody>
      <% @contents.each do |line| %>
        <tr>
          <td></td>
          <td><%= line %></td>
          <td><%= line %></td>
          <td><%= line %></td>
          <td><%= line %></td>
          <td><%= line %></td>
          <td><%= line %></td>
          <td><%= line %></td>
          <td><%= line %></td>
          <td><%= line %></td>
          <td><%= line %></td>
        </tr>
      <% end %>
    </tbody>
  </table>
  </section>

</section>